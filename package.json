{
  "name": "@myrotvorets/envalidators",
  "version": "2.1.1",
  "description": "Collection of validators for envalid",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint",
    "test": "mocha test",
    "test:coverage": "c8 mocha test",
    "posttest": "npm run build",
    "prepare": "npm run build"
  },
  "files": [
    "dist/lib/index.d.ts",
    "dist/lib/index.js",
    "dist/lib/index.js.map",
    "dist/lib/*/index.d.ts",
    "dist/lib/*/index.js",
    "dist/lib/*/index.js.map"
  ],
  "typings": "dist/lib/index.d.ts",
  "keywords": [
    "envalid",
    "validator",
    "validators"
